{
  "name": "dasa-lab-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "NODE_PATH=./build node build/bootstrap",
    "start:watch": "nodemon",
    "start:typescript": "NODE_PATH=./src ts-node --pretty --project 'tsconfig.json' src/bootstrap",
    "debug": "NODE_PATH=./src nodemon --inspect src/bootstrap.ts",
    "test": "jest --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint:staged": "lint-staged",
    "format:staged": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:staged && npm run format:staged"
    }
  },
  "lint-staged": {
    "!(*test).ts": [
      "tslint -c 'tslint.json' -p 'tsconfig.json' -t codeFrame  --fix "
    ]
  },
  "keywords": [],
  "author": "gustavolizze",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/module-alias": "^2.0.0",
    "@types/mongoose": "5.10.2",
    "@types/node": "^13.13.5",
    "@types/pino": "^6.3.5",
    "@types/reflect-metadata": "^0.1.0",
    "@types/source-map-support": "^0.5.3",
    "@types/throng": "^5.0.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-extended": "^0.11.5",
    "lint-staged": "^10.2.2",
    "mockingoose": "^2.13.2",
    "moq.ts": "^7.0.0",
    "nodemon": "^2.0.3",
    "pino-pretty": "^4.3.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.19",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@typegoose/typegoose": "^7.4.7",
    "class-validator": "^0.13.0",
    "fast-glob": "^3.2.4",
    "fastify": "^3.9.2",
    "fastify-compress": "^3.4.1",
    "fastify-cors": "^5.1.0",
    "fastify-helmet": "^5.1.0",
    "lodash": "^4.17.19",
    "make-promises-safe": "^5.1.0",
    "mongoose": "5.10.18",
    "pino": "^6.10.0",
    "reflect-metadata": "^0.1.13",
    "throng": "^5.0.0"
  }
}
